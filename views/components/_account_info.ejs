<% if (clientData.account, clientData.client, clientData.adress, clientData.place) { %>
    <%= clientData.client.firstName %>
    <article class="badge">
    <h4>Hello <%= clientData.client.firstName %></h4>
        <p>Here you can find your personal data</p> 
    </article>
    <article class="card">
        <section class="card-body">
            <h3 class="card-title">This is you</h3>
            <p class="card-text"><%= clientData.client.firstName %> <%= clientData.client.lastName %></p>
            <p class="card-text"><%= clientData.adress.street %> <%= clientData.adress.houseNr %> <%= clientData.adress.bus %></p>
            <p class="card-text"><%= clientData.place.zipCode %> <%= clientData.place.placeName %></p>
            <p class="card-text"><%= clientData.account.email %></p>        
        </section>
        <section class="card-footer">
            <a href="" class="card-link">Change personal data</a>
            <a href="" class="card-link">Change password</a>
        </section>
    </article>    
<% } else { %>
    <p class="badge badge-danger">We are sorry, we coudn't find your personal data.</p>
<% } %>

